{
  "name": "logoscon/replicast",
  "description": "Replicate content across WordPress installs via the WP REST API.",
  "type": "wordpress-plugin",
  "keywords": ["wordpress", "plugin"],
  "homepage": "http://log.pt/",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "log.OSCON, Lda.",
      "email": "engenharia@log.pt",
      "homepage": "http://log.pt/"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    },
    {
      "type": "composer",
      "url": "https://composer.log.pt"
    },
    {
      "type": "vcs",
      "url": "https://github.com/s3rgiosan/WP-Dismissible-Notices-Handler"
    }
  ],
  "support": {
    "issues": "https://bitbucket.org/log-oscon/replicast/issues",
    "source": "https://bitbucket.org/log-oscon/replicast/src"
  },
  "autoload": {
    "psr-4": {
      "Replicast\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Replicast\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "5.5"
    }
  },
  "require": {
    "guzzlehttp/guzzle": "6.1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "*@stable",
    "apigen/apigen": "@stable",
    "10up/wp_mock": "dev-master"
  },
  "suggest": {}
}
